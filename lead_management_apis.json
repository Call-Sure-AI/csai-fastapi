      {
        "name": "Leads CRUD",
        "item": [
          {
            "name": "Get All Leads",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{auth_token}}",
                  "type": "text"
                }
              ],
              "url": {
                "raw": "{{base_url}}/api/leads?page=1&page_size=50",
                "host": ["{{base_url}}"],
                "path": ["api", "leads"],
                "query": [
                  {"key": "page", "value": "1"},
                  {"key": "page_size", "value": "50"},
                  {"key": "status", "value": "new", "disabled": true},
                  {"key": "source", "value": "csv_import", "disabled": true},
                  {"key": "priority", "value": "high", "disabled": true},
                  {"key": "score_min", "value": "50", "disabled": true},
                  {"key": "score_max", "value": "100", "disabled": true},
                  {"key": "search_query", "value": "", "disabled": true}
                ]
              }
            }
          },
          {
            "name": "Get Lead by ID",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{auth_token}}",
                  "type": "text"
                }
              ],
              "url": {
                "raw": "{{base_url}}/api/leads/{{lead_id}}",
                "host": ["{{base_url}}"],
                "path": ["api", "leads", "{{lead_id}}"]
              }
            }
          },
