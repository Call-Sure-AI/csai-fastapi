      {
        "name": "Import",
        "item": [
          {
            "name": "Import CSV",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{auth_token}}",
                  "type": "text"
                }
              ],
              "body": {
                "mode": "formdata",
                "formdata": [
                  {
                    "key": "file",
                    "type": "file",
                    "src": ""
                  }
                ]
              },
              "url": {
                "raw": "{{base_url}}/api/leads/import/csv",
                "host": ["{{base_url}}"],
                "path": ["api", "leads", "import", "csv"]
              }
            }
          },
          {
            "name": "Import API",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{auth_token}}",
                  "type": "text"
                },
                {
                  "key": "Content-Type",
                  "value": "application/json",
                  "type": "text"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n    \"source\": \"api\",\n    \"data\": [\n        {\n            \"email\": \"test@example.com\",\n            \"first_name\": \"Test\",\n            \"last_name\": \"User\",\n            \"phone\": \"+1234567890\",\n            \"company\": \"Test Corp\"\n        }\n    ]\n}"
              },
              "url": {
                "raw": "{{base_url}}/api/leads/import/api",
                "host": ["{{base_url}}"],
                "path": ["api", "leads", "import", "api"]
              }
            }
          }
        ]
