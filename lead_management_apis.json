              ],
              "body": {
                "mode": "raw",
                "raw": "{\n    \"lead_ids\": [\"{{lead_id}}\"],\n    \"updates\": {\n        \"status\": \"qualified\",\n        \"priority\": \"high\"\n    }\n}"
              },
              "url": {
                "raw": "{{base_url}}/api/leads/bulk-update",
                "host": ["{{base_url}}"],
                "path": ["api", "leads", "bulk-update"]
